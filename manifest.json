{
    "manifest_version": 3,
    "name": "Open URL Extension",
    "version": "1.0",
    "description": "An extension to open a URL",
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    }
}